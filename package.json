{
    "name": "be-circle",
    "version": "0.0.1",
    "description": "Awesome project developed with TypeORM.",
    "type": "commonjs",
    "devDependencies": {
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@babel/preset-typescript": "^7.23.3",
        "@cld-apis/types": "^0.1.6",
        "@types/amqplib": "^0.10.4",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^1.4.11",
        "@types/node": "^16.11.10",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "ts-node": "10.9.1",
        "typescript": "4.5.2"
    },
    "dependencies": {
        "amqplib": "^0.10.3",
        "bcrypt": "^5.1.1",
        "cloudinary": "^2.0.1",
        "cloudinary-build-url": "^0.2.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.1",
        "express": "^4.18.2",
        "joi": "^17.12.1",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.4.0",
        "redis": "^4.6.13",
        "reflect-metadata": "^0.1.13",
        "typeorm": "0.3.20"
    },
    "scripts": {
        "test": "jest",
        "start": "ts-node src/index.ts",
        "dev": "nodemon --exec ts-node src/index.ts",
        "typeorm": "typeorm-ts-node-commonjs",
        "worker": "nodemon --exec ts-node src/workers/index.ts",
        "migrate:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/migrations/My-Migration -d ./src/data-source.ts",
        "migrate:run": "npx typeorm-ts-node-commonjs migration:run -d ./src/data-source.ts",
        "schema:drop": "npx typeorm-ts-node-commonjs schema:drop -d ./src/data-source.ts"
    }
}
